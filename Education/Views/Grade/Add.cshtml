@using Education.ViewModel;
@{
    ViewData["Title"] = "Add";
    @model Grade;
}

<h1>Add Grade</h1>

<form asp-action="Add" method="post">

    <partial name="_AddGradePartial"  for="@Model" />
    <div class="form-group">
        <input type="submit" value="Add" class="btn btn-primary" />
    </div>
</form>
<div>
    <button class="btn btn-primary"><a style="color:white;text-decoration:none" asp-action="GetAll">Back To Grades</a> </button>
</div>

<partial name="_ValidationScriptsPartial" />
